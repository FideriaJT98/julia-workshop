{
  "pipeline": {
    "name": "model"
  },
  "transform": {
    "image": "dwhitena/julia-train:tree",
    "cmd": [ 
	"julia", 
	"/train.jl",
	"/pfs/training/iris.csv", 
	"/pfs/out/model.jld"	
    ]
  },
  "parallelism_spec": {
    "strategy": "CONSTANT",
    "constant": "1"
  },
  "input": {
    "atom": {
      "repo": "training",
      "glob": "/"
    }
  }
}
